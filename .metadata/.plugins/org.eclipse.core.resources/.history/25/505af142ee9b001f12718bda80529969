package Practise;

abstract class Book{
	public abstract void getBookNmae();
	public void science() {
		System.out.println("Science Book");
	}
}
class Note extends Book{

	@Override
	public void getBookNmae() {
		System.out.println("Math Book");
		
	}
	
}


public class Day5_AbstractClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Book b = new Note();
		b.science();
		b.getBookNmae();
		

	}

}
